/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding:0;
  box-sizing: border-box;
} 

input{font-size: 32rpx}
text{font-size: 28rpx}
button{
  font-size: 36rpx;
  background: #F44336;
  color: #fff;
  border-radius: 8rpx;
  transition: all .3s;
  letter-spacing: 1rpx;
}
button:active{
  background: rgba(244,67,54,.5);
  color: #fff;
}
button::after{
  border: 0 none;
}
.no_more{
  text-align: center;
  font-size: 30rpx;
  color: #757575;
  position: relative;
  padding-top: 20rpx;
  padding-bottom: 40rpx;
}
.no_more text{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
  padding: 0 20rpx;
  background: #f4f4f4;
  z-index: 2;
}
.no_more::before,.no_more::after{
  content: '';
  position: absolute;
  top: 52%;
  background: #BDBDBD;
  width: 50%;
  height: 1rpx;
  z-index: 0
}
.no_more::before{
  left: 40rpx;
}
.no_more::after{
  right: 40rpx;
}
.no_data{
  position:fixed;
  top:0;
  bottom:0;
  width:100%;
  display:flex;
  align-items:center;
  justify-content:center;
}
.no_data icon{
  font-size:200rpx;
  color: #999999;
}

